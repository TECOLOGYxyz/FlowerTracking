file = {:C$\backslash$:/Users/au309263/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Fang et al. - 2012 - Molecular cloning, characterization and expression of goose Toll-like receptor 5.pdf:pdf},
isbn = {0161-5890},
issn = {1872-9142},
journal = {Mol Immunol},
keywords = {*Cloning,Amino Acid Sequence,Animal/*immunology,Animals,Bird Diseases/*immunology,Cell Line,Flagellin/genetics/*immunology,Geese/genetics/*immunology,Gene Expression Regulation,HEK293 Cells,Humans,Interferon-gamma/biosynthesis,Interleukin-18/biosynthesis,Interleukin-6/biosynthesis,Interleukin-8/biosynthesis,Messenger/biosynthesis,Molecular,Molecular Sequence Data,NF-kappa B/genetics,Open Reading Frames/genetics,RNA,Salmonella Infections,Salmonella enteritidis/immunology,Salmonella typhimurium/immunology,Sequence Alignment,Spleen/metabolism,Toll-Like Receptor 5/biosynthesis/chemistry/*genet,bacterial flagellin,cutting edge,cytokine expression,enterica serovar enteritidis,expression,functional-characterization,gene-expression,goose,host-defense,immune-responses,messenger-rna expression,race,salmonella enteritidis,salmonella-enteritidis,toll-like receptor},
