title = {{Toll-like receptors in innate immunity}},
url = {http://www.ncbi.nlm.nih.gov/entrez/query.fcgi?cmd=Retrieve{\&}db=PubMed{\&}dopt=Citation{\&}list{\_}uids=15585605{\%}5Cnhttp://intimm.oxfordjournals.org/cgi/reprint/17/1/1.pdf},
