file = {:C$\backslash$:/Users/au309263/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Kannaki et al. - 2015 - Differential Toll-Like Receptor (TLR) mRNA Expression Patterns during Chicken Embryological Development.pdf:pdf},
issn = {1532-2378},
journal = {Animal biotechnology},
keywords = {chicken embryo,gene expression,poultry,real-time pcr tlrs},
number = {2},
pages = {130--5},
pmid = {25380465},
title = {{Differential Toll-Like Receptor (TLR) mRNA Expression Patterns during Chicken Embryological Development.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/25380465},
volume = {26},
year = {2015}
}
