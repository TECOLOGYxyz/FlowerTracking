@article{Phillips1986,
abstract = {A simple inexpensive method of determining level of ploidy in fish using silver-stained cell preparations is evaluated. The method involves determining the maximum number of nucleoli per cell on silver-stained slides. Any tissue can be used since whole cells, rather than chromosomes are required for analysis. Since the amount of tissue needed to make slides is very small, samples may be obtained from fish about 7-8 cm long without sacrificing the animal. Results from three salmonid species (rainbow trout, chinook salmon and coho salmon) showed that haploid individuals had one nucleolus/cell, diploid individuals had one or two nucleoli/cell and triploid individuals had 1, 2 or 3 nucleoli/cell. The fraction of cells with three nucleoli/cell was higher in triploid embryos (averaging 75{\%}) than in gill tissue from triploid fish of 6 months or 2 years of age (averaging 43{\%} and 36{\%}, respectively). However, no cells with three nucleoli were ever found in diploid individuals, making identification of triploids unambiguous. This method should be applicable to most fish species since the majority of those examined have been shown to have only one chromosome with a nucleolar organizer region (NOR) per haploid genome. {\textcopyright} 1986.},
author = {Phillips, Ruth B. and Zajicek, Kerry D. and Ihssen, Peter E. and Johnson, Orlay},
doi = {10.1016/0044-8486(86)90275-9},
file = {:C$\backslash$:/Users/au309263/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Phillips et al. - 1986 - Application of silver staining to the identification of triploid fish cells.pdf:pdf},
issn = {00448486},
journal = {Aquaculture},
number = {4},
pages = {313--319},
title = {{Application of silver staining to the identification of triploid fish cells}},
volume = {54},
year = {1986}
}
@article{Piertney2005,
abstract = {The major histocompatibility complex (MHC) has become a paradigm for how selection can act to maintain adaptively important genetic diversity in natural populations. Here, we review the contribution of studies on the MHC in non-model species to our understanding of how selection affects MHC diversity, emphasising how ecological and ethological processes influence the tempo and mode of evolution at the MHC, and conversely, how variability at the MHC affects individual fitness, population dynamics and viability. We focus on three main areas: the types of information that have been used to detect the action of selection on MHC genes; the relative contributions of parasite-mediated and sexual selection on the maintenance of MHC diversity; and possible future lines of research that may help resolve some of the unanswered issues associated with MHC evolution.},
author = {Piertney, S.B. and Oliver, M.K.},
doi = {10.1038/sj.hdy.6800724},
file = {:C$\backslash$:/Users/au309263/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Piertney, Oliver - 2005 - The evolutionary ecology of the major histocompatibility complex.pdf:pdf},
isbn = {0018-067X (Print) 0018-067X (Linking)},
issn = {0018-067X},
journal = {Heredity},
keywords = {Biological Evolution,Ecology,Genetic,Genetic Variation,Humans,Major Histocompatibility Complex,Major Histocompatibility Complex: genetics,Population Dynamics,Reproduction,Selection},
