journal = {European Wind Energy Association Annual Conference and Exhibition 2015, EWEA 2015 - Scientific Proceedings},
keywords = {Bird detection ecological conservation,Image recognition,Social acceptance},
pages = {104--107},
title = {{Evaluation of bird detection using time-lapse images around a wind farm}},
year = {2015}
}
@article{Mortensen2016,
abstract = {As a response to current climate changes, individual species have$\backslash$nchanged various biological traits, illustrating an inherent phenotypic$\backslash$nplasticity. However, as species are embedded in an ecological network$\backslash$ncharacterised by multiple consumer-resource interactions, ecological$\backslash$nmismatches are likely to arise when interacting species do not respond$\backslash$nhomogeneously. The approach of biological networks analysis calls for$\backslash$nthe use of structural equation modelling (SEM), a multidimensional$\backslash$nanalytical setup that has proven particularly useful for analysing$\backslash$nmultiple interactions across trophic levels. Here we apply SEM to a$\backslash$nlong-term dataset from a High-Arctic ecosystem to analyse how$\backslash$nphenological responses across three trophic levels are coupled to$\backslash$nsnowmelt patterns and how changes may cascade through consumer-resource$\backslash$ninteractions. Specifically, the model included the effect of snowmelt on$\backslash$na High-Arctic tri-trophic system of flowers, insects and waders$\backslash$n(Charadriiformes), with latent factors representing phenology (timing of$\backslash$nlife history events) and performance (abundance or reproduction success)$\backslash$nfor each trophic level. The effects derived from the model demonstrated$\backslash$nthat the time of snowmelt directly affected plant and arthropod$\backslash$nphenology as well as the performance of all included trophic levels.$\backslash$nAdditionally, timing of snowmelt appeared to indirectly influence wader$\backslash$nphenology as well as plant, arthropod and wader performance through$\backslash$neffects on adjacent trophic levels and lagged effects. The results from$\backslash$nthe tri-trophic community presented here emphasise that effects of$\backslash$nclimate on species in consumer-resource systems may propagate through$\backslash$ntrophic levels.},
author = {Mortensen, Lars O. and Schmidt, Niels Martin and H{\o}ye, Toke T. and Damgaard, Christian and Forchhammer, Mads C.},
doi = {10.1007/s00300-015-1872-z},
file = {:C$\backslash$:/Users/au309263/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Mortensen et al. - 2016 - Analysis of trophic interactions reveals highly plastic response to climate change in a tri-trophic High-Arcti.pdf:pdf},
isbn = {0722-4060, 1432-2056},
issn = {07224060},
journal = {Polar Biology},
keywords = {Arctic ecosystem,Arthropods,Greenland,Performance,Phenology,Plants,Structural equation modelling,Trophic interactions,Trophic mismatch,Waders},
number = {8},
pages = {1467--1478},
publisher = {Springer Berlin Heidelberg},
title = {{Analysis of trophic interactions reveals highly plastic response to climate change in a tri-trophic High-Arctic ecosystem}},
volume = {39},
year = {2016}
}
@article{Kang2011,
abstract = {Innate immune receptors respond to common structural patterns in microbial molecules and are called pattern recognition receptors. Toll-like receptors (TLRs) play critical roles in the innate immune system by recognizing microbial lipids, carbohydrates, nucleic acids, and proteins. Precise definition of the ligand "pattern" of TLRs has been difficult to determine primarily owing to a lack of high-resolution structures. Recently, the structures of several TLR-ligand complexes and the intracellular signaling domains have been determined by X-ray crystallography. This new structural information, combined with extensive biochemical and immunological data accumulated over decades, sheds new light on ligand-recognition and -activation mechanisms. In this review, we summarize the TLR structures and discuss proposed ligand-recognition and -activation mechanisms.},
