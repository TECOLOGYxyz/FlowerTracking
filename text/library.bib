journal = {Molecular Ecology},
keywords = {adaptation,captive breeding,embryo losses,ex situ management,heterozygosity,pedigrees},
number = {8},
pages = {1645--1653},
pmid = {25735639},
title = {{Impacts of early viability selection on management of inbreeding and genetic diversity in conservation}},
volume = {24},
year = {2015}
}
