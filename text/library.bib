file = {:C$\backslash$:/Users/au309263/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Mikami et al. - 2012 - Molecular evolution of vertebrate Toll-like receptors Evolutionary rate difference between their leucine-rich rep.pdf:pdf},
isbn = {0378-1119},
issn = {03781119},
journal = {Gene},
keywords = {Evolutionary rate,Gene conversion,Intracellular Toll IL-receptor domain,Leucine rich repeats,Platypus,TLR family},
number = {2},
pages = {235--243},
pmid = {22587897},
title = {{Molecular evolution of vertebrate Toll-like receptors: Evolutionary rate difference between their leucine-rich repeats and their TIR domains}},
volume = {503},
year = {2012}
