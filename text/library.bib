issn = {12979686},
journal = {Genetics Selection Evolution},
number = {1},
pages = {1--16},
publisher = {BioMed Central},
title = {{Cost-benefit analysis of aquaculture breeding programs}},
url = {https://doi.org/10.1186/s12711-018-0372-3},
volume = {50},
year = {2018}
}
