issn = {1664-8021},
journal = {Frontiers in Genetics},
keywords = {Doubled haploid,Paralogous Sequence Variants,SNP discovery,genome resequencing,rainbow trout},
number = {April},
pages = {147},
title = {{A New Single Nucleotide Polymorphism Database for Rainbow Trout Generated Through Whole Genome Resequencing}},
