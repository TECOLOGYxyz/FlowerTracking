journal = {G3: Genes, Genomes, Genetics},
number = {5},
pages = {795--806},
pmid = {23550126},
title = {{Distribution of long-range linkage disequilibrium and Tajima's D Values in scandinavian populations of Norway Spruce ({\textless}i{\textgreater}Picea abies{\textless}/i{\textgreater})}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=3656727{\&}tool=pmcentrez{\&}rendertype=abstract{\%}5Cnhttp://g3journal.org/cgi/doi/10.1534/g3.112.005462},
