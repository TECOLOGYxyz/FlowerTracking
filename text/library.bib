issn = {2041-210X},
journal = {Methods in Ecology and Evolution},
keywords = {centre for ecology,conservation,university of exeter},
number = {June},
pages = {1773--1782},
title = {{Using radiotelemetry to study behavioural thermoregulation in insects under field conditions}},
