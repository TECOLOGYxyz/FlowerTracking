isbn = {1932-6203 (Electronic)$\backslash$r1932-6203 (Linking)},
issn = {1932-6203},
journal = {PloS one},
keywords = {Animals,Endangered Species,Exons,Gene Duplication,Genetic,Genotype,Immunity, Innate,Immunity, Innate: genetics,Innate,Innate: genetics,Microsatellite Repeats,New Zealand,Polymorphism, Single Nucleotide,Selection, Genetic,Single Nucleotide,Songbirds,Songbirds: genetics,Songbirds: immunology,Toll-Like Receptor 7,Toll-Like Receptor 7: genetics,Toll-Like Receptors,Toll-Like Receptors: genetics,Toll-Like Receptors: immunology},
